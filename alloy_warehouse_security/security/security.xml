<odoo>
    <data>

        <record model="ir.rule" id="stock_only_warehouse_comp_rule">
            <field name="name">Warehouse only company</field>
            <field name="model_id" ref="stock.model_stock_picking_type"/>
            <!--<field name="global" eval="True"/>-->
            <field name="groups" eval="[(4, ref('alloy_warehouse_security.warehouse_admin'))]"/>
            <field name="domain_force">[('warehouse_id','=', user.warehouse_id.id)]</field>
        </record>


        <record id="stock.stock_picking_type_rule" model="ir.rule">
            <!--<field name="name">Stock Operation Type multi-companyyyyyyyyyyyyyy</field>-->
            <!--<field name="model_id" ref="stock.model_stock_picking_type"/>-->
            <!--<field name="global" eval="True"/>-->
            <!--<field name="active" eval="False"/>-->
            <field name="domain_force">[]</field>
        </record>

        <record id="warehouse_admin" model="res.groups">
            <field name="name">warehouse administrator</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="warehouse_admin_rule" model="ir.rule">
            <field name="name">warehouse administrator</field>
            <field name="model_id" ref="stock.model_stock_picking_type"/>
            <field name="domain_force">[(1,'=',1)]</field>
        </record>

        <record id="project_user_limit" model="res.groups">
            <field name="name">project limitation</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <!--<record id="project_admin_rule" model="ir.rule">-->
            <!--<field name="name">project administrator</field>-->
            <!--<field name="model_id" ref="project.model_project_project"/>-->
            <!--<field name="groups" eval="[(4, ref('alloy_warehouse_security.project_user_limit'))]"/>-->
            <!--<field name="domain_force">[(1,'=',1)]</field>-->
        <!--</record>-->

        <record model="ir.rule" id="project_only_limitation_rule">
            <field name="name">project limitation</field>
            <field name="model_id" ref="project.model_project_project"/>
            <field name="groups" eval="[(4, ref('alloy_warehouse_security.project_user_limit'))]"/>
            <field name="domain_force">[('name','=', user.project_id.name)]</field>
        </record>


    </data>
</odoo>